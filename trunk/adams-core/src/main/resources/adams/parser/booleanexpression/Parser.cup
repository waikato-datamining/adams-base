                :}
              | ISNAN LPAREN expr:e RPAREN
                {: RESULT = parser.getHelper().toDouble(e).isNaN(); :}
              | LENGTH LPAREN expr:s RPAREN
                {: 
                   if (s.getClass().isArray())
