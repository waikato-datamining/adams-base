adams.flow.control.Flow -annotation "Loads predictions from a CSV file and generates a density-based scatter plot using JFreeChart." -execute-on-error adams.flow.control.postflowexecution.Null -execute-on-finish adams.flow.control.postflowexecution.Null -flow-execution-listener adams.flow.execution.NullListener -flow-restart-manager adams.flow.control.flowrestart.NullManager
 adams.flow.source.FileSupplier -file ${EXAMPLE_FLOWS}/data/fried_predictions.csv -use-forward-slashes true
 adams.flow.transformer.SpreadSheetFileReader -reader "adams.data.io.input.CsvSpreadSheetReader -data-row-type adams.data.spreadsheet.DenseDataRow -spreadsheet-type adams.data.spreadsheet.DefaultSpreadSheet -parse-formulas false"
 adams.flow.sink.JFreeChartPlot -display-type adams.flow.core.displaytype.Default -width 1200 -height 800 -writer adams.gui.print.NullWriter -dataset "adams.gui.visualization.jfreechart.dataset.DefaultXY -add-diagonal-series true" -chart "adams.gui.visualization.jfreechart.chart.DensityScatterPlot -title \"LinearRegression: fried dataset\" -label-x actual -label-y predicted -mode KDE -generator \"adams.gui.visualization.core.BiColorGenerator -first-color #0000cc -second-color #ff9900 -alpha 127\"" -shape adams.gui.visualization.jfreechart.shape.Default -color-provider adams.gui.visualization.core.DefaultColorProvider -watermark adams.gui.visualization.watermark.Default