# Configurations for various table classes and other related classes.
#
# Author: FracPete (fracpete at waikato dot ac dot nz)

# Outputs a stacktrace in case the initTable call for table fails (false|true)
InitTables.Debug=false

# Whether to use ANSI quotes, i.e., double quotes, around table and column names
AnsiQuotes=false

# The prefix to use for table names
TablePrefix=

# The logging table
adams.db.mysql.LogT.Debug=false
adams.db.postgresql.LogT.Debug=false
adams.db.sqlite.LogT.Debug=false
